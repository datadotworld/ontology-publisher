   <owl:NamedIndividual rdf:about="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/MetadataSECDebtAssetBacked/DebtAssetBackedModule">
      <rdf:type rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/Module"/>
      <rdfs:label>FIBO SEC Asset-Backed Debt Module</rdfs:label>
      <dct:abstract>Asset backed securities have income payments derived from and backed (collateralized) by a specified pool of underlying assets which are generally small and illiquid and unable to be sold individually.</dct:abstract>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/AssetBackedSPVs/"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/AssetBackedSecurities/"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/CBOs/"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/CDOs/"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/CLOs/"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/CMOs/"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/MortgageBackedSecurities/"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/PoolBackedSecurities/"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/SyntheticCDOs/"/>
      <dct:license rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://opensource.org/licenses/MIT</dct:license>
