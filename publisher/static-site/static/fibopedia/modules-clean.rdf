   <owl:NamedIndividual rdf:about="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/MetadataSECDebt/DebtModule">
      <rdf:type rdf:resource="http://www.omg.org/techprocess/ab/SpecificationMetadata/Module"/>
      <rdfs:label>FIBO SEC Debt Module</rdfs:label>
      <dct:abstract>This module defines debt securities contracts both cash and synthetic, such as bonds, structured finance instruments, short term or money market instruments, and any other contract type characterized by the holding of some debt of the issuer or primary party by the holder or counterparty.</dct:abstract>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AssetBacked/MetadataSECDebtAssetBacked/DebtAssetBackedModule"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/Bonds/MetadataSECDebtBonds/DebtBondsModule"/>
      <dct:hasPart rdf:resource="https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtFoundations/MetadataSECDebtDebtFoundations/DebtDebtFoundationsModule"/>
